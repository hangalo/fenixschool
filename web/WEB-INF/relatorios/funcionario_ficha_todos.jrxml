	<queryString>
		<![CDATA[SELECT fenixschoolem.funcionario.id_funcionario,
	fenixschoolem.funcionario.nome_funcionario,
	fenixschoolem.funcionario.sobrenome_funcionario,
	fenixschoolem.funcionario.sexo,
	fenixschoolem.funcionario.data_nascimento,
	fenixschoolem.funcionario.casa_funcionario,
	fenixschoolem.funcionario.bairro_funcionario,
	fenixschoolem.funcionario.distrito_funcionario,
	fenixschoolem.funcionario.foto_funcionario,
	fenixschoolem.funcionario.telefone_movel,
	fenixschoolem.funcionario.telefone_fixo,
	fenixschoolem.funcionario.email_funcionario,
	fenixschoolem.municipio.nome_municipio,
	fenixschoolem.provincia.nome_provincia
FROM fenixschoolem.funcionario
	INNER JOIN fenixschoolem.municipio ON 
	 fenixschoolem.funcionario.id_municipio = fenixschoolem.municipio.id_municipio 
	INNER JOIN fenixschoolem.provincia ON 
	 fenixschoolem.municipio.id_provincia = fenixschoolem.provincia.id_provincia]]>
	</queryString>
